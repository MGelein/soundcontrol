{
  "name": "soundcontrol",
  "version": "1.0.0",
  "description": "DND Ambient Sound Control",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack:osx": "electron-packager . SoundControl --out=dist/osx --platform=darwin --arch=x64 --icon=build/icon.icns",
    "pack:win64": "electron-packager . SoundControl --out=dist/ --icon=build/icon.ico",
    "build": "npm run pack:osx && npm run pack:win64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MGelein/soundcontrol.git"
  },
  "author": "Mees Gelein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MGelein/soundcontrol/issues"
  },
  "homepage": "https://github.com/MGelein/soundcontrol#readme",
  "devDependencies": {
    "electron": "^8.1.1",
    "electron-packager": "^14.2.1"
  },
  "dependencies": {
    "electron-builder": "^22.4.0"
  }
}
